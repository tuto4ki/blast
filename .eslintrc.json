{
  "parser": "@babel/eslint-parser",
  "plugins": ["prettier"],
  "extends": [
    "plugin:prettier/recommended",
    "prettier",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "class-methods-use-this": 0
  }
}
